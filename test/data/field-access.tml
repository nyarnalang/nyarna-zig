=== Field access
--- input
\declare:
  Data = \record:
    a, b : \Raw;
  \end(record)
\end(declare)

\declare:
  foo = \func:
    data : \Data;
  :body:
    (\data::a, \data::b)
  \end(func)
\end(declare)

\foo(\Data(x, y))
--- ast
+CALL
  +TARGET
    =SYMREF .doc.foo
  -TARGET
  +PARAM "data"
    +CALL
      +TARGET
        =SYMREF .doc.Data
      -TARGET
      +PARAM "a"
        =LIT TEXT "x"
      -PARAM
      +PARAM "b"
        =LIT TEXT "y"
      -PARAM
    -CALL
  -PARAM
-CALL
--- document
=RAW Raw "(x, y)"