=== Field access
--- input
\declare:
  Data = \record:
    a, b : \Raw;
  \end(record)
\end(declare)

\declare:
  foo = \func:
    data : \Data;
  :body:
    (\data::a, \data::b)
  \end(func)
\end(declare)

\foo(\Data(x, y))
--- ast
+CALL
  +TARGET
    =SYMREF .doc.foo
  -TARGET
  +PROTO "data"
    +CALL
      +TARGET
        =SYMREF .doc.Data
      -TARGET
      +PROTO "a"
        =LIT TEXT "x"
      -PROTO
      +PROTO "b"
        =LIT TEXT "y"
      -PROTO
    -CALL
  -PROTO
-CALL
--- document
=RAW Raw "(x, y)"